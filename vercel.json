{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "astro",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "trailingSlash": true,
  "cleanUrls": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "DENY" },
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" }
      ]
    },
    {
      "source": "/_astro/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=86400" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/restaurants", "destination": "/restaurants/" },
    { "source": "/about", "destination": "/about/" },
    { "source": "/cart", "destination": "/cart/" },
    { "source": "/checkout", "destination": "/checkout/" },
    { "source": "/history", "destination": "/history/" },
    { "source": "/terms", "destination": "/terms/" },
    { "source": "/restaurants/cupsncrumbs", "destination": "/restaurants/cupsncrumbs/" },
    { "source": "/restaurants/aaviora", "destination": "/restaurants/aaviora/" },
    { "source": "/restaurants/arsalan", "destination": "/restaurants/arsalan/" },
    { "source": "/restaurants/bandhu-hotel", "destination": "/restaurants/bandhu-hotel/" },
    { "source": "/restaurants/bholebaba", "destination": "/restaurants/bholebaba/" },
    { "source": "/restaurants/whitechocolate", "destination": "/restaurants/whitechocolate/" }
  ]
}
